<%= simple_form_for(@search) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :keywords %>
    <%= f.input :attribute_id %>
    <%= f.input :min_price %>
    <%= f.input :max_price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
